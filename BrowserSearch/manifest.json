{
  "manifest_version": 3,
  "name": "Tab and Bookmark Search",
  "version": "1.0",
  "permissions": [
    "tabs",
    "bookmarks",
    "history"
  ],
  "action": {
    "default_popup": "popup.html" //,
    //"default_icon": {
    //  "16": "icon.png",
    //  "48": "icon.png",
    //  "128": "icon.png"
    //}
  },
  "icons": {
    //"16": "icon.png",
    //"48": "icon.png",
    //"128": "icon.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      },
      "description": "Search"
    }
  }
}
